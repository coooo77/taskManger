        "nameYouDoNotWantToApply"
      ],
      "handleFolder": "E:\\path\\to\\handle",
      "includeExt": [
        "mp4"
      ],
      "keepFiles": false,
      "ffmpegSetting": "-vcodec libx264 -crf 24 -preset veryfast",
      "suffix": "_compress",
      "outputFolder": "E:\\path\\to\\output",
      "screenshot": true,
      "skip": false,
      "sourceFolder": [
        "E:\\path\\to\\folder1",
        "E:\\path\\to\\folder2"
      ],
      "split": true,
      "type": "convert"
    },
    {
      "exceptions": [
        "nameYouDoNotWantToApply"
      ],
      "includeExt": [
        "mp4",
        "jpg"
      ],
      "skip": false,
      "sourceFolder": [
        "E:\\temp"
      ],
      "targetFolder": "D:\\JD\\test\\source",
      "type": "move"
    },
    {
      "addThumbnail": true,
      "exceptions": [],
      "handleFolder": "E:\\path\\to\\handle",
      "includeExt": [
        "mp4",
        "jpg"
      ],
      "keepFiles": false,
      "outputFolder": "E:\\path\\to\\output",
      "skip": false,
      "sourceFolder": [
        "E:\\path\\to\\folder1",
        "E:\\path\\to\\folder2"
      ],
      "type": "upload",
      "uploadAsDraft": true
    }
  ],
  "screenshot": {
    "outputFolder": "",
    "count": 10,
    "timestamp": [
      30,
      "00:00:30"
    ],
    "interval": 10800
  },
  "convert": {
    "ext": "mp4",
    "defaultFFmpegSetting": "-b 350k -filter:v setpts=PTS/4 -vcodec libx264 -crf 24 -an -preset veryfast",
    "defaultSuffix": "_mute_convert_4x_crf24",
    "crf": 28,
    "showConvertCmd": true,
    "customSetting": {
      "nameOfStreamer": {
        "ffmpegSetting": "-b 350k -filter:v setpts=PTS/4 -vcodec libx264 -crf 18 -an -preset veryfast",
        "suffix": "_mute_convert"
      }
    }
  },
  "combine": {
    "suffixForCombine": "_combine",
    "fileNameClipper": "_",
    "showCombineCmd": true
  },
  "split": {
    "splitIntervalInSec": 3600,
    "showSplitCmd": true
  },
  "upload": {
    "streamListPath": "A:\\path\\to\\steamList.json",
    "skipWhenDownloadReach": 5,
    "credentials": {
      "email": "yourEmail@example.com",
      "pass": "yourPassword",
      "recoveryemail": "recoveryemail@gmail.com"
    },
    "puppeteerSetting": {
      "executablePath": "A:\\Program Files\\Google\\Chrome\\Application\\chrome.exe",
      "headless": true
    },
    "showProgress": false
  }
